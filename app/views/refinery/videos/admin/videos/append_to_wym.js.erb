parent.$.fn.getIndex = function(){
    var $p=$(this).parent().children();
    return $p.index(this);
};
var selectedPart = parent.$('#page_parts .ui-tabs-active').getIndex();
var wym = parent.$.wymeditors(selectedPart);
var selectedBlock = wym.selected();
var html = '<%= escape_javascript(@html_for_wym) %>';
if (selectedBlock !== null) {
  $(selectedBlock).append(html);
} else {
    wym.html(wym.html() + html);
}
wym.update();
wym.close_dialog();
close_dialog();

