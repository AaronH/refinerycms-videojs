parent.$.fn.getIndex = function(){
    var $p=$(this).parent().children();
    return $p.index(this);
}
selectedPart = parent.$('#page_parts .ui-tabs-active').getIndex();
wym = parent.$.wymeditors(selectedPart);
selectedBlock = wym.selected();
html = "<%= render(partial: "show_video", locals: {video: @video}) %>"
if (selectedBlock !== null) {
  $(selectedBlock).append(html);
} else {
    wym.html(wym.html() + html);
}
wym.update();
wym.close_dialog();
close_dialog();

